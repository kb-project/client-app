<Page
    x:Class="IdentifyApp.FinalPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:IdentifyApp"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="#FFE570">

        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>            
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>

        <StackPanel Grid.Row="0" Background="White">
            <Image Source="Images/kblogo.png" Height="50"/>
        </StackPanel>
        <StackPanel Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center">
            <!--<TextBlock Text="Final Page 입니다" Foreground="#81776B" FontSize="16" Margin="10"/>-->
            <Image Source="Images/final.PNG" Stretch="UniformToFill"/>
        </StackPanel>
        
        <StackPanel Grid.Row="2" Background="White">
            <Image Source="Images/kblogo2.png" Height="50" />
        </StackPanel>
    </Grid>
</Page>
